{{#unless burgers.length }}
<h3>No Burgers Yet</h3>
{{/unless}}
<ul class="burger_list" id="list-{{side}}">
  {{#each burgers}}
  <li class="listItem" data-id="{{this.id}}">
    <span>{{this.id}} - {{this.burger_name}}</span>
    {{#unless this.devoured }}
    <button class="devour" type="button">DEVOUR</button>
    {{/unless}}
  </li>
  {{/each}}
</ul>